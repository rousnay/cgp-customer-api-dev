{
  "version": 1,
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.js",
      "methods": [
            "GET",
            "POST",
            "PUT",
            "PATCH",
            "DELETE"
      ]
    }
  ]
}